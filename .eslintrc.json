{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["airbnb-base", "plugin:prettier/recommended"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"rules": {
		"import/extensions": [
			"error",
			"ignorePackages",
			{
				"js": "never",
				"jsx": "never",
				"ts": "never",
				"tsx": "never"
			}
		],
		"consistent-return": "off",
		"prettier/prettier": "error"
	}
}
